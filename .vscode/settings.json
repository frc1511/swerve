{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "memory": "cpp",
    "__functional_base": "cpp",
    "array": "cpp",
    "istream": "cpp",
    "locale": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "*.tcc": "cpp",
    "functional": "cpp",
    "future": "cpp"
  }
}
